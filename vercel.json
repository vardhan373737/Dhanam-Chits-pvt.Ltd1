{
  "version": 2,
  "builds": [
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "rewrites": [
    { "source": "/login", "destination": "/login.html" },
    { "source": "/chitty-plans", "destination": "/chitty-plans.html" }
  ]
}
